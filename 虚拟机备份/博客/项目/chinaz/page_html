<script type="text/javascript">
		/*$(function(){
			$('#firLinkBar-arraw').on('click', function(){
				if($('#morefriend').height() == '80'){
					$('#morefriend').css({'height' : 'auto'});
				}else{
					$('#morefriend').css({'height' : '80'});
				}
