  <script>
	 	$(document).ready(function(){
		 $(".pla-close").click(function(){
				   $(this).parent().slideUp("slow");
				   }); 

			   	//判断是否宽屏
				var winWide = window.screen.width;
				var wideScreen = false; 
				var mainWidth = 1020;
				//var mainWidth = 1040;
				if(wideScreen){
					mainWidth = 1020;
				}else{
					mainWidth = 1020;
				//	mainWidth = 1040;
				}
				
				goTopInit(mainWidth);
				$(window).resize(function(){goTopInit(mainWidth);});
			
				
				 $(window).scroll(function(){
            goTopInit(mainWidth);
            if(!$(window).scrollTop() == 0){
                $(".img_wrap_l").css({
                    "top":350
                });
			}else{
                $(".img_wrap_l").css({
                    "top":300
                });
			}

        });
			 
				function goTopInit(mainWidth){ 
					var winWide = $(window).width();   
					$(".img_wrap_l").css({
						"left":(winWide-mainWidth)/2-120
					});
					$(".img_wrap_r").css({
						"left":(winWide-mainWidth)/2+mainWidth
					}); 					
				} 
		});

	 </script> 
 
